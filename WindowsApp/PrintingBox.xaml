<Window x:Class="Naga.PrintingBox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PrintingBox" Height="250" Width="450" WindowStartupLocation="CenterScreen" WindowStyle="None"  >
    <Grid Background="LightGreen">
        <Label x:Name="lblone" Content="Label Printing..." HorizontalAlignment="Left" Foreground="Red" FontStyle="Normal" FontFamily="Arial" FontSize="25" FontWeight="Bold" Margin="80,49,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lbltwo" Content="Place Another Bag....." HorizontalAlignment="Left" Foreground="Red" FontStyle="Normal" FontFamily="Arial" FontSize="25" FontWeight="Bold" Margin="80,111,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btnOk" Content="OK"  FontSize="20" HorizontalAlignment="Left" Margin="320,178,0,0" VerticalAlignment="Top" Width="98" Height="36" Background="#6E6E6E" FontWeight="Bold" Click="btnOk_Click"/>

    </Grid>
</Window>
